algoritmo "SomarValores"
// Pratica 01
// Somar valores
var
     v1, v2:Inteiro
     
Funcao soma(a, b:Inteiro):Inteiro
inicio
         Retorne a + b
FimFuncao
inicio
      Escreval("Digite o primeiro valor: ")
      Leia(v1)
      Escreval("Digite o segundo valor: ")
      Leia(v2)
      
      Escreval("O resultado e ", soma(v1, v2))

fimalgoritmo
